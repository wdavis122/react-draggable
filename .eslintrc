{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "quotes": ["single"],
    "curly": "multi-line",
    "camelcase": 0,
    "comma-dangle": 1,
    "no-use-before-define": "nofunc",
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "new-cap": 0,
    "semi": 1
  },
  env: {
    "browser": true,
    "node": true
  }
}
